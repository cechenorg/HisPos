                    <StackPanel x:Name="OTCTicketDis" Grid.Row="1" Grid.Column="6" Background="Transparent" Orientation="Horizontal" PreviewMouseDown="OTCTicketDis_PreviewMouseDown">
                        <StackPanel.InputBindings>
                            <MouseBinding Command="{Binding TradeProfitTicketReportSelectionChangedCommand}" Gesture="LeftClick" />
                        </StackPanel.InputBindings>
                        <Border BorderBrush="Gray">
                            <Label Width="130" VerticalAlignment="Center" HorizontalContentAlignment="Right" Content="{Binding  TradeDetailReportSum.DiscountAmtMinus, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ContentStringFormat="{}{0:N0}" Padding="0,0,30,0" />
                        </Border>
                    </StackPanel>
                    <StackPanel x:Name="OTCRewardEmp" Grid.Row="1" Grid.Column="7" Background="Transparent" Orientation="Horizontal" PreviewMouseDown="OTCRewardEmp_PreviewMouseDown">
                        <StackPanel.InputBindings>
                            <MouseBinding Command="{Binding RewardReportSelectionChangedCommand}" Gesture="LeftClick" />
                        </StackPanel.InputBindings>
                        <Border BorderBrush="Gray">
                            <Label Width="130" HorizontalContentAlignment="Right" Content="{Binding  TotalRewardReport.RewardAmount, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ContentStringFormat="{}{0:N0}" Padding="0,0,30,0" />
                        </Border>
                    </StackPanel>

                    <StackPanel Grid.Row="1" Grid.Column="8" Background="Transparent" Orientation="Horizontal" PreviewMouseDown="OTCTrade_PreviewMouseDown">
                        <Border BorderBrush="Gray" BorderThickness="0,0,0,0">
                            <Label Width="200" VerticalAlignment="Center" HorizontalContentAlignment="Right" Content="{Binding TradeDetailReportSum.TotalProfit, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ContentStringFormat="{}{0:N0}" Padding="0,0,80,0" />
                        </Border>
                    </StackPanel>

                    <StackPanel Grid.Row="1" Grid.Column="9" Background="Transparent" Orientation="Horizontal" PreviewMouseDown="OTCTrade_PreviewMouseDown">
                        <Border BorderBrush="Gray" BorderThickness="0,0,0,0">
                            <Label Width="200" HorizontalContentAlignment="Right" Content="-" ContentStringFormat="{}{0:N0}" Padding="0,0,70,0" />
                        </Border>
                    </StackPanel>


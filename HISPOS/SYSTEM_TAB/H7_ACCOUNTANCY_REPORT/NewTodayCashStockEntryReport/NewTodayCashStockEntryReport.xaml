        <service:NullableDateConverter x:Key="NullableDateConverter" />
        <DataTemplate x:Key="CashDetailControl" DataType="{x:Type newTodayCashStockEntryReport:NewTodayCashStockEntryReportViewModel}">
            <entryDetailControl:CashDetailControl />
        </DataTemplate>
        <DataTemplate x:Key="TodayPrescriptionDetailControl" DataType="{x:Type newTodayCashStockEntryReport:NewTodayCashStockEntryReportViewModel}">
            <entryDetailControl:TodayPrescriptionDetailControl />
        </DataTemplate>
        <DataTemplate x:Key="StockTakingDetailControl" DataType="{x:Type newTodayCashStockEntryReport:NewTodayCashStockEntryReportViewModel}">
            <entryDetailControl:StockTakingDetailControl />
        </DataTemplate>
        <DataTemplate x:Key="StockTakingOTCDetailControl" DataType="{x:Type newTodayCashStockEntryReport:NewTodayCashStockEntryReportViewModel}">
            <entryDetailControl:StockTakingOTCDetailControl />
        </DataTemplate>
        <DataTemplate x:Key="TradeProfitDetailControl" DataType="{x:Type newTodayCashStockEntryReport:NewTodayCashStockEntryReportViewModel}">
            <entryDetailControl:TradeProfitDetailControl />
        </DataTemplate>
        <DataTemplate x:Key="ExtraMoneyDetailControl" DataType="{x:Type newTodayCashStockEntryReport:NewTodayCashStockEntryReportViewModel}">
            <entryDetailControl:ExtraMoneyDetailControl />
        </DataTemplate>
        <DataTemplate x:Key="RewardDetailControl" DataType="{x:Type newTodayCashStockEntryReport:NewTodayCashStockEntryReportViewModel}">
            <entryDetailControl:RewardDetailControl />
        </DataTemplate>
        <DataTemplate x:Key="DepositDetailControl" DataType="{x:Type newTodayCashStockEntryReport:NewTodayCashStockEntryReportViewModel}">
            <newTodayEntryDetailControl:DepositDetailControl />
        </DataTemplate>

    </UserControl.Resources>
    <xctk:BusyIndicator BusyContent="{Binding BusyContent, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" IsBusy="{Binding IsBusy, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}">

                        <StackPanel.InputBindings>
                            <MouseBinding Command="{Binding ViewReportDetailCommand}" CommandParameter="{x:Static newTodayCashStockEntryReport:NewTodayCashStockEntryReportViewModel+ReportDetailType.AllPrescription_Count}" Gesture="LeftClick" />
                        </StackPanel.InputBindings>
                        <Border BorderBrush="Gray" BorderThickness="0,0,0,0">
                            <Label Width="130" HorizontalContentAlignment="Right" Content="{Binding PrescriptionDetailReportSumMain.MedTotalCount, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ContentStringFormat="{}{0:N0}" Padding="0,0,30,0" />
                        </Border>
                    </StackPanel>
                    <StackPanel x:Name="PREIncome" Grid.Row="2" Grid.Column="2" Background="Transparent" Orientation="Horizontal" PreviewMouseDown="PREIncome_PreviewMouseDown">

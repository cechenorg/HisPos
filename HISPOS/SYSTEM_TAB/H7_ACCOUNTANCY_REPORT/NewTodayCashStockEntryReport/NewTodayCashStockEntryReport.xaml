                    <StackPanel x:Name="OTCAll" Grid.Row="1" Grid.Column="1" Background="Transparent" Orientation="Horizontal" PreviewMouseDown="OTCAll_PreviewMouseDown">
                        <StackPanel.InputBindings>
                            <MouseBinding Command="{Binding TradeProfitAllReportSelectionChangedCommand}" Gesture="LeftClick" />
                        </StackPanel.InputBindings>
                        <Border BorderBrush="Gray" BorderThickness="0,0,0,0">
                            <Label Width="130" HorizontalContentAlignment="Right" Content="{Binding TradeDetailReportSum.Count, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ContentStringFormat="{}{0:N0}" Padding="0,0,30,0" />
                        </Border>
                    </StackPanel>
                    <StackPanel x:Name="OTCIncome" Grid.Row="1" Grid.Column="2" Background="Transparent" Orientation="Horizontal" PreviewMouseDown="OTCIncome_PreviewMouseDown">
                        <StackPanel.InputBindings>
                            <MouseBinding Command="{Binding TradeProfitIcomeReportSelectionChangedCommand}" Gesture="LeftClick" />
                        </StackPanel.InputBindings>
                        <Border BorderBrush="Gray">
                            <Label Width="130" HorizontalContentAlignment="Right" Content="{Binding TradeDetailReportSum.RealTotal, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ContentStringFormat="{}{0:N0}" Padding="0,0,30,0" />
                        </Border>
                    </StackPanel>
                    <StackPanel x:Name="OTCCost" Grid.Row="1" Grid.Column="3" Background="Transparent" Orientation="Horizontal" PreviewMouseDown="OTCCost_PreviewMouseDown_1">
                        <StackPanel.InputBindings>
                            <MouseBinding Command="{Binding TradeProfitCostCostReportSelectionChangedCommand}" Gesture="LeftClick" />
                        </StackPanel.InputBindings>
                        <Border BorderBrush="Gray">
                            <Label Width="130" HorizontalContentAlignment="Right" Content="{Binding TradeDetailReportSum.TotalCost, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ContentStringFormat="{}{0:N0}" FontFamily="Segoe UI Semibold" FontSize="20" Padding="0,0,30,0" />
                        </Border>
                    </StackPanel>
                    <StackPanel x:Name="OTCChange" Grid.Row="1" Grid.Column="4" Background="Transparent" Orientation="Horizontal" PreviewMouseDown="OTCChange_PreviewMouseDown">
                        <StackPanel.InputBindings>
                            <MouseBinding Command="{Binding TradeChangeReportSelectionChangedCommand}" Gesture="LeftClick" />
                        </StackPanel.InputBindings>
                        <Border BorderBrush="Gray">
                            <Label Width="130" HorizontalContentAlignment="Right" Content="{Binding  TradeDetailReportSum.TotalChange, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ContentStringFormat="{}{0:N0}" Padding="0,0,30,0" />
                        </Border>
                    </StackPanel>
                    <StackPanel x:Name="OTCStockChange" Grid.Row="1" Grid.Column="5" Background="Transparent" Orientation="Horizontal" PreviewMouseDown="OTCStockChange_PreviewMouseDown">
                        <StackPanel.InputBindings>

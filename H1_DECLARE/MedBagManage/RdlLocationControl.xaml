<UserControl
    x:Class="His_Pos.H4_BASIC_MANAGE.MedBagManage.RdlLocationControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:His_Pos.H4_BASIC_MANAGE.MedBagManage"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <Grid IsHitTestVisible="False" Background="{Binding GridBackground}">
        <Label
            Name="RdlParameterName"
            Margin="0,-20,0,-20"
            HorizontalAlignment="Left"
            VerticalAlignment="Center"
            FontFamily="Segoe UI Semibold"
            Content="{Binding LabelContent}"
            FontSize="10" />
    </Grid>
</UserControl>
